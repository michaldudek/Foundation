{
    "name": "michaldudek/foundation",
    "description": "A set of useful PHP classes.",
    "keywords": ["tools", "utils", "array", "object", "string"],
    "homepage": "http://www.michaldudek.pl/Foundation/",
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Pałys-Dudek",
            "email": "michal@michaldudek.pl"
        }
    ],
    "require": {
        "php": ">=5.6,<7.2",
        "psr/log": ">=1.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.6",
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd" : "~2.2"
    },
    "extra": {
        "branch-alias": {
            "dev-release/0.11": "0.11.x-dev"
        }
    },
    "autoload": {
    	"psr-0": {
            "MD\\Foundation\\Tests": "tests/",
            "MD\\Foundation": "src/"
        },
        "files": [
            "src/constants.php",
            "src/MD/functions.php"
        ]
    }
}
